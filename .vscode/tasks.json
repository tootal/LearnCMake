{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "CMake Build",
			"command": "cmake",
			"args": [
				"-S",
				".",
				"-B",
				"build",
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$msCompile"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}